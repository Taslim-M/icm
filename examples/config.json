{
  "search_params": {
    "alpha": 100.0,
    "initial_temperature": 3.0,
    "final_temperature": 0.001,
    "cooling_rate": 0.98,
    "initial_examples": 20,
    "max_iterations": 1000,
    "consistency_fix_iterations": 10
  },
  "model_params": {
    "generation_temperature": 0.2,
    "generation_top_p": 0.9,
    "generation_max_tokens": 512
  },
  "system_params": {
    "device": "auto",
    "seed": 42,
    "log_level": "INFO"
  },
  "export_params": {
    "default_formats": ["json", "dpo"],
    "include_stats": true,
    "create_pairs": true
  },
  "dataset_params": {
    "default_sample_size": 1000,
    "default_split": "train"
  }
}
